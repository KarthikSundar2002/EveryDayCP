{
  "name": "cpday",
  "version": "1.0.0",
  "description": "A desktop app to show a cp algorithm and related problems",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
